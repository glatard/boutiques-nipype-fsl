        {
            "command-line-flag": "--out_z_name",
            "name": "Out z name",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_Z_NAME]",
            "type": "String",
            "id": "input_out_z_name",
            "description": "A file name. Output file name for z-stats (either as txt or image."
