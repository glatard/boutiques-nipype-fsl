            "list": false,
            "command-line-key": "[MAX_RESTART]",
            "type": "Number",
            "id": "input_max_restart",
            "description": "An integer (int or long). Maximum number of restarts."
        },
        {
            "command-line-flag": "--maxit",
            "name": "Maxit",
            "optional": true,
            "list": false,
            "command-line-key": "[MAXIT]",
            "type": "Number",
            "id": "input_maxit",
            "description": "An integer (int or long). Maximum number of iterations before restart."
        },
        {
            "command-line-flag": "--mix",
            "name": "Mix",
            "optional": true,
            "list": false,
            "command-line-key": "[MIX]",
            "type": "File",
            "id": "input_mix",
            "description": "An existing file name. Mixing matrix for mixture modelling / filtering."
        },
        {
            "command-line-flag": "--mm_thresh",
            "name": "Mm thresh",
            "optional": true,
            "list": false,
            "command-line-key": "[MM_THRESH]",
            "type": "Number",
            "id": "input_mm_thresh",
            "description": "A float. Threshold for mixture model based inference."
        },
        {
            "command-line-flag": "--no_bet",
            "name": "No bet",
            "optional": true,
            "list": false,
            "command-line-key": "[NO_BET]",
            "type": "String",
            "id": "input_no_bet",
            "description": "A boolean. Switch off bet."
        },
        {
            "command-line-flag": "--no_mask",
            "name": "No mask",
            "optional": true,
            "list": false,
            "command-line-key": "[NO_MASK]",
            "type": "String",
            "id": "input_no_mask",
            "description": "A boolean. Switch off masking."
        },
        {
            "command-line-flag": "--no_mm",
            "name": "No mm",
            "optional": true,
            "list": false,
            "command-line-key": "[NO_MM]",
            "type": "String",
            "id": "input_no_mm",
            "description": "A boolean. Switch off mixture modelling on ic maps."
        },
        {
            "command-line-flag": "--non_linearity",
            "name": "Non linearity",
            "optional": true,
            "list": false,
            "command-line-key": "[NON_LINEARITY]",
            "type": "String",
            "id": "input_non_linearity",
            "description": "A string. Nonlinearity: gauss, tanh, pow3, pow4."
        },
        {
            "command-line-flag": "--num_ICs",
            "name": "Num ics",
            "optional": true,
            "list": false,
            "command-line-key": "[NUM_ICS]",
            "type": "Number",
            "id": "input_num_ICs",
            "description": "An integer (int or long). Number of ic's to extract (for deflation approach)."
