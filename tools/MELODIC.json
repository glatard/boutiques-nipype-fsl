        {
            "command-line-flag": "--var_norm",
            "name": "Var norm",
            "optional": true,
            "list": false,
            "command-line-key": "[VAR_NORM]",
            "type": "String",
            "id": "input_var_norm",
            "description": "A boolean. Switch off variance normalization."
