        {
            "command-line-flag": "--log_power",
            "name": "Log power",
            "optional": true,
            "list": false,
            "command-line-key": "[LOG_POWER]",
            "type": "String",
            "id": "input_log_power",
            "description": "A boolean. Calculate log of power for frequency spectrum."
        },
        {
            "command-line-flag": "--mask",
            "name": "Mask",
            "optional": true,
            "list": false,
            "command-line-key": "[MASK]",
            "type": "File",
            "id": "input_mask",
            "description": "An existing file name. File name of mask for thresholding."
