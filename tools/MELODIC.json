        {
            "command-line-flag": "--out_all",
            "name": "Out all",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_ALL]",
            "type": "String",
            "id": "input_out_all",
            "description": "A boolean. Output everything."
