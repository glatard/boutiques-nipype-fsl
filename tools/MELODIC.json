        {
            "command-line-flag": "--out_orig",
            "name": "Out orig",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_ORIG]",
            "type": "String",
            "id": "input_out_orig",
            "description": "A boolean. Output the original ics."
        },
        {
            "command-line-flag": "--out_pca",
            "name": "Out pca",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_PCA]",
            "type": "String",
            "id": "input_out_pca",
            "description": "A boolean. Output pca results."
        },
        {
            "command-line-flag": "--out_stats",
            "name": "Out stats",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_STATS]",
            "type": "String",
            "id": "input_out_stats",
            "description": "A boolean. Output thresholded maps and probability maps."
        },
        {
            "command-line-flag": "--out_unmix",
            "name": "Out unmix",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_UNMIX]",
            "type": "String",
            "id": "input_out_unmix",
            "description": "A boolean. Output unmixing matrix."
        },
        {
            "command-line-flag": "--out_white",
            "name": "Out white",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_WHITE]",
            "type": "String",
            "id": "input_out_white",
            "description": "A boolean. Output whitening/dewhitening matrices."
