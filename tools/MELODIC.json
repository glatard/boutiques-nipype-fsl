        {
            "command-line-flag": "--t_con",
            "name": "T con",
            "optional": true,
            "list": false,
            "command-line-key": "[T_CON]",
            "type": "File",
            "id": "input_t_con",
            "description": "An existing file name. T-contrast matrix across time-domain."
