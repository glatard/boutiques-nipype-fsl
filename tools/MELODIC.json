            "list": false,
            "command-line-key": "[REPORT]",
            "type": "String",
            "id": "input_report",
            "description": "A boolean. Generate melodic web report."
        },
        {
            "command-line-flag": "--report_maps",
            "name": "Report maps",
            "optional": true,
            "list": false,
            "command-line-key": "[REPORT_MAPS]",
            "type": "String",
            "id": "input_report_maps",
            "description": "A string. Control string for spatial map images (see slicer)."
        },
        {
            "command-line-flag": "--s_con",
            "name": "S con",
            "optional": true,
            "list": false,
            "command-line-key": "[S_CON]",
            "type": "File",
            "id": "input_s_con",
            "description": "An existing file name. T-contrast matrix across subject-domain."
        },
        {
            "command-line-flag": "--s_des",
            "name": "S des",
            "optional": true,
            "list": false,
            "command-line-key": "[S_DES]",
            "type": "File",
            "id": "input_s_des",
            "description": "An existing file name. Design matrix across subject-domain."
        },
        {
            "command-line-flag": "--sep_vn",
            "name": "Sep vn",
            "optional": true,
            "list": false,
            "command-line-key": "[SEP_VN]",
