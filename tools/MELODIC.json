        {
            "command-line-flag": "--tr_sec",
            "name": "Tr sec",
            "optional": true,
            "list": false,
            "command-line-key": "[TR_SEC]",
            "type": "Number",
            "id": "input_tr_sec",
            "description": "A float. Tr in seconds."
