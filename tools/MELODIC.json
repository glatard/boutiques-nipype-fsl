{
    "inputs": [
        {
            "command-line-flag": "--ICs",
            "name": "Ics",
            "optional": true,
            "list": false,
            "command-line-key": "[ICS]",
            "type": "File",
            "id": "input_ICs",
            "description": "An existing file name. Filename of the ic components file for mixture modelling."
        },
        {
            "command-line-flag": "--approach",
            "name": "Approach",
            "optional": true,
            "list": false,
            "command-line-key": "[APPROACH]",
            "type": "String",
            "id": "input_approach",
            "description": "A string. Approach for decomposition, 2d: defl, symm (default),  3d: tica (default), concat."
        },
        {
            "command-line-flag": "--args",
            "name": "Args",
            "optional": true,
            "list": false,
            "command-line-key": "[ARGS]",
            "type": "String",
            "id": "input_args",
            "description": "A string. Additional parameters to the command."
        },
        {
            "command-line-flag": "--bg_image",
            "name": "Bg image",
            "optional": true,
            "list": false,
            "command-line-key": "[BG_IMAGE]",
            "type": "File",
            "id": "input_bg_image",
            "description": "An existing file name. Specify background image for report (default: mean image)."
        },
        {
            "command-line-flag": "--bg_threshold",
            "name": "Bg threshold",
            "optional": true,
            "list": false,
            "command-line-key": "[BG_THRESHOLD]",
            "type": "Number",
            "id": "input_bg_threshold",
            "description": "A float. Brain/non-brain threshold used to mask non-brain voxels, as a percentage (only if --nobet selected)."
        },
        {
            "command-line-flag": "--cov_weight",
            "name": "Cov weight",
            "optional": true,
            "list": false,
            "command-line-key": "[COV_WEIGHT]",
            "type": "Number",
            "id": "input_cov_weight",
            "description": "A float. Voxel-wise weights for the covariance matrix (e.g. segmentation information)."
        },
        {
            "command-line-flag": "--dim",
            "name": "Dim",
            "optional": true,
            "list": false,
            "command-line-key": "[DIM]",
            "type": "Number",
            "id": "input_dim",
            "description": "An integer (int or long). Dimensionality reduction into #num dimensions(default: automatic estimation)."
        },
        {
            "command-line-flag": "--dim_est",
            "name": "Dim est",
            "optional": true,
            "list": false,
            "command-line-key": "[DIM_EST]",
            "type": "String",
            "id": "input_dim_est",
            "description": "A string. Use specific dim. estimation technique: lap, bic, mdl, aic, mean (default: lap)."
        },
        {
            "command-line-flag": "--environ",
            "name": "Environ",
            "default-value": "{}",
            "optional": true,
            "list": false,
            "command-line-key": "[ENVIRON]",
            "type": "String",
            "id": "input_environ",
            "description": "A dictionary with keys which are a value of type 'str' and with values which are a value of type 'str'. Environment variables."
        },
        {
            "command-line-flag": "--epsilon",
            "name": "Epsilon",
            "optional": true,
            "list": false,
            "command-line-key": "[EPSILON]",
            "type": "Number",
            "id": "input_epsilon",
            "description": "A float. Minimum error change."
        },
        {
            "command-line-flag": "--epsilonS",
            "name": "Epsilons",
            "optional": true,
            "list": false,
            "command-line-key": "[EPSILONS]",
            "type": "Number",
            "id": "input_epsilonS",
            "description": "A float. Minimum error change for rank-1 approximation in tica."
        },
        {
            "command-line-flag": "--ignore_exception",
            "name": "Ignore exception",
            "default-value": "False",
            "optional": true,
            "list": false,
            "command-line-key": "[IGNORE_EXCEPTION]",
            "type": "String",
            "id": "input_ignore_exception",
            "description": "A boolean. Print an error message instead of throwing an exception in case the interface fails to run."
        },
        {
            "command-line-flag": "--in_files",
            "name": "In files",
            "optional": false,
            "list": true,
            "command-line-key": "[IN_FILES]",
            "type": "File",
            "id": "input_in_files",
            "description": "A list of items which are an existing file name. Input file names (either single file name or a list)."
