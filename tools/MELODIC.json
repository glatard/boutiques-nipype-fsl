        {
            "command-line-flag": "--out_dir",
            "name": "Out dir",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_DIR]",
            "type": "String",
            "id": "input_out_dir",
            "description": "A directory name. Output directory name."
        },
        {
            "command-line-flag": "--out_mean",
            "name": "Out mean",
            "optional": true,
            "list": false,
            "command-line-key": "[OUT_MEAN]",
            "type": "String",
            "id": "input_out_mean",
            "description": "A boolean. Output mean volume."
